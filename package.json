{
  "name": "cordova-minify-obfuscate",
  "version": "0.0.1",
  "description": "Cordova hook that uglifies and minifies your app's Javascript, CSS and HTML files, as well as compresses your image files.",
  "homepage": "https://github.com/yeejfe/cordova-minify-obfuscate",
  "keywords": [
    "cordova",
    "uglify",
    "minify",
    "hook",
    "hooks",
    "cordova-minify",
    "cordova-minify-v2",
    "cordova-minify-obfuscate"
  ],
  "dependencies" : {
    "clean-css": "4.1.x",
    "html-minifier": "3.5.x",
    "imagemin": "5.3.x",
    "imagemin-gifsicle": "5.2.x",
    "imagemin-jpegtran": "5.0.x",
    "imagemin-optipng": "5.2.x",
    "imagemin-svgo": "6.0.x",
    "uglify-js": "3.1.x"
  },
  "author": "yeejfe",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository" : {
    "type": "git",
    "url": "https://github.com/yeejfe/cordova-minify-obfuscate.git"
  },
  "scripts": {
    "postinstall": "node scripts/install.js",
    "postuninstall": "node scripts/uninstall.js"
  }
}
